! appearance
URxvt*depth:            32
URxvt*background:       #263238
URxvt*foreground:       #FFFFFF
URxvt.underlineColor:   #4682B4
URxvt.highlightColor:   #4682B4
URxvt.throughColor:     Blue
URxvt*cursorColor:      #C5C8C6
URxvt*cursorColor2:     #Black
URxvt*internalBorder:   15
URxvt*externalBorder:   0
URxvt.colorIT:          #87AF5F
URxvt.colorBD:          #C5C8C6
URxvt.colorUL:          #87AFD7
URxvt*saveLines:        12000
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false
URxvt*scrollstyle:      plain
URxvt*cursorBlink:      true
URxvt*cursorUnderline:  true

xterm*dynamicColors: true
urxvt*dynamicColors: on

! colors
URxvt*color0:   #25283D
URxvt*color1:   #ecdf8e
URxvt*color2:   #07bee8
URxvt*color3:   #84dcc6
URxvt*color4:   #0a8754
URxvt*color5:   #af3b6e
URxvt*color6:   #cbf30f
URxvt*color7:   #edddd4
URxvt*color8:   #709080
URxvt*color9:   #ee2e31
URxvt*color10:  #e83f6f
URxvt*color11:  #f0dfaf
URxvt*color12:  #04bff3
URxvt*color13:  #ffbf00
URxvt*color14:  #1cfeba
URxvt*color15:  #ffffff


! fonts
URxvt*font:           xft:ctrld:size=15
URxvt*imFont:         xft:ctrld:size=15
URxvt*boldFont:       xft:ctrld:size=15
URxvt*italicFont:     xft:ctrld:size=15
URxvt*boldItalicFont: xft:ctrld:size=15

!urxvt extensions:
URxvt.perl-ext-common:  default,matcher,tabbedex,clipboard

!enabling clickable links:
URxvt.urlLauncher:      /usr/bin/
URxvt.matcher.button:   1 

! urxvt tabs settings:
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title: noUrxvt.color12: rgb:5c/5c/ff

URxvt.iso14755: False
URxvt.keysym.Control-C: perl:clipboard:copy
URxvt.keysym.Control-V: perl:clipboard:paste
